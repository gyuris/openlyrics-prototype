<?xml version="1.0" encoding='UTF-8'?>
<?xml-stylesheet href="stylesheets/openlyrics.css" type="text/css"?>
<song xmlns="http://openlyrics.info/namespace/2009/song" xml:lang="hu" version="0.9" chordnotation="hungarian" createdIn="Geany" modifiedIn="Geany" modifiedDate="2019-02-27T17:47:03">
  <properties>
    <titles>
      <title lang="hu">A kapudat nyisd meg</title>
    </titles>
    <verseOrder>v1 v2</verseOrder>
    <authors>
      <author>ismeretlen</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>
        A <chord root="D">kapudat nyisd meg,</chord> <chord root="G">Jézusnak nyisd meg,</chord> <chord root="D">jön, közele</chord><chord root="A">dik</chord>.<br/>
        A <chord root="D">kapudat nyisd meg,</chord> <chord root="G">Jézusnak nyisd meg,</chord> <chord root="D">jön,</chord> <chord root="A">közele</chord><chord root="D">dik</chord>.<br/>
      </lines>
      <lines>
        <chord root="D">Nyisd</chord> <chord root="G">meg, íme,</chord> <chord root="D">jön, közele</chord><chord root="A">dik</chord>.<br/>
        <chord root="D">Nyisd</chord> <chord root="G">meg, íme,</chord> <chord root="D">jön,</chord> <chord root="A">közele</chord><chord root="D">dik</chord>.
      </lines>
    </verse>
    <verse name="v2">
      <lines>
        A <chord root="D">kapudat zárd be,</chord> <chord root="G">kapudat zárd be, az</chord> <chord root="D">Úr Jézus u</chord><chord root="A">tán</chord>.<br/>
        A <chord root="D">kapudat zárd be,</chord> <chord root="G">kapudat zárd be, az</chord> <chord root="D">Úr</chord> <chord root="A">Jézus u</chord><chord root="D">tán</chord>.<br/>
      </lines>
      <lines>
        <chord root="D">Zárd</chord> <chord root="G">be és ne</chord> <chord root="D">engededd el soha töb</chord><chord root="A">bé</chord>.<br/>
        <chord root="D">Zárd</chord> <chord root="G">be és ne</chord> <chord root="D">engededd el</chord> <chord root="A">soha töb</chord><chord root="D">bé</chord>.
      </lines>
    </verse>
  </lyrics>
</song>
